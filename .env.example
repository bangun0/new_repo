# TodayPickup API Client 환경변수 설정 예시

# TodayPickup API 설정
TODAYPICKUP_BASE_URL=https://admin.todaypickup.com
TODAYPICKUP_API_PREFIX=/api

# 기본 인증 정보 (선택사항)
DEFAULT_AGENCY_ID=your_agency_id_here
DEFAULT_AUTH_TOKEN=your_auth_token_here

# CORS 설정
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:8080"]

# HTTP 클라이언트 설정
HTTP_TIMEOUT=30
HTTP_RETRIES=3

# 로깅 설정
LOG_LEVEL=INFO